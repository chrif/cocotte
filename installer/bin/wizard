#!/usr/bin/env php
<?php
require __DIR__.'/../vendor/autoload.php';

use Chrif\Cocotte\DependencyInjection\Application;
use Symfony\Component\Console\Input\InputInterface;
use Symfony\Component\Console\Output\OutputInterface;

$app = new Application(
    __DIR__.'/../config/services_wizard.yml'
);
$app->console()->setDefaultCommand('wizard', true);
$app->console()->run(
    $app->container()->get(InputInterface::class),
    $app->container()->get(OutputInterface::class)
);
