#!/usr/bin/env php
<?php
require __DIR__.'/../vendor/autoload.php';

use Chrif\Cocotte\DependencyInjection\Application;

putenv('MACHINE_IP=' . exec("sh /installer/machine-ip"));

$app = new Application(
    __DIR__.'/../config/services.yml',
    __DIR__.'/../config/cocotte.yml'
);

$app->console()->run();